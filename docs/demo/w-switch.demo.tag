<w-switch-demo>
    <h1>w-switch</h1>

    <h2>Default</h2>
    <div data-is="w-switch"></div>

    <h2>Checked</h2>
    <div data-is="w-switch" class="checked"></div>

    <h2>Color</h2>
    <div data-is="w-switch" style="color:blue;"></div>

    <h2>Disabled</h2>
    <div data-is="w-switch" class="disabled"></div>

    <h2>Disabled and Checked</h2>
    <div data-is="w-switch" class="disabled checked"></div>
</w-switch-demo>